#include <stdio.h>
#include <stdbool.h>

int main()
{
    int senha = 1234 ;
    int comp;
   
    printf("Digite uma senha");
    scanf  ("%d", &comp);
    while (comp != senha ) {
        printf ("Senha errada ");
      printf("Digite novamente");
      scanf ("%d",&comp);
        
        
}
 printf("Senha correta");
    return 0;
}

