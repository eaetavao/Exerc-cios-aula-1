#include <stdio.h>

int main()
{
    int idade;
    int contador = 0;
    int pessoas = 0;

    printf("Digite a idade de 1 a 10 :\n");

    while (pessoas < 5) {
        printf("Pessoa %d: ", pessoas + 1);
        scanf("%d", &idade);

        if (idade > 18) {
            contador++;
        }

        pessoas++;
    }

    printf("NÃºmero de pessoas com mais de 18 anos: %d\n", contador);

    return 0;
